{
    "name": "6card/ddd",
    "autoload": {
        "psr-4": {
            "App\\": "src/",
            "App\\Presentation\\Admin\\": "admin/src/Presentation/",
            "App\\Presentation\\Front\\": "front/src/Presentation/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "6card",
            "email": "vk_31@mail.ru"
        }
    ],
    "require": {
        "yiisoft/yii2": "^2.0"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer": true,
            "pestphp/pest-plugin": true
        }
    },
    "require-dev": {
        "yiisoft/yii2-debug": "^2.1",
        "pestphp/pest": "^3.8"
    }
}
